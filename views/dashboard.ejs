<%- include header %>
		<% jobs.forEach(function(val, key) {%>
		<div class="container-fluid">
			<div class="row">
				<div class="col-xs-9">
					<h4><a href="<%= val.link %>"><%= val.title %></a></h4>
				</div>
				<div class="col-xs-3">
					<span style="float:right; font-size: 0.7rem;"><%= val.posted%></span>
				</div>				
			</div>
			<div class="row">
				<div class="col-xs-12">
					<div class="blurb"><%= val.link %></div>
				</div>			
			</div>
			<div class="row">
				<div class="col-xs-9">
					<span class="tag tag-success"><%= val.payment%></span>
				</div>
				<div class="col-xs-3">
					<span class="fa fa-star-o pull-right" onClick="reply_(this.id)" id='favorite_<%= key %>'></span>
				</div>
			</div>
		</div>
		 <% }); %>

<%- include footer %>